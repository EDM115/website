  "scripts": {
    "analyze": "pnpm wasm && nuxt analyze",
    "build": "pnpm wasm && nuxt build",

    "cleanup": "nuxt cleanup",

    "dev": "nuxt dev --no-clear --no-fork",
    "dev:expose": "nuxt dev --no-clear --no-fork --host",

    "generate": "pnpm wasm && nuxt generate",

    "lint": "run-p lint:oxlint lint:eslint",
    "lint:fix": "run-s lint:fix:oxlint lint:fix:eslint",

    "lint:eslint": "eslint --cache --cache-location .eslintcache --concurrency=auto .",
    "lint:fix:eslint": "eslint --concurrency=auto --fix .",

    "lint:oxlint": "oxlint --type-aware .",
    "lint:fix:oxlint": "oxlint --fix --type-aware .",

    "format": "eslint -c eslint.stylistic.config.ts --concurrency=auto --fix .",

    "preview": "nuxt preview --port 8000",
    "start:ssr": "node .output/server/index.mjs",
    "start:ssg": "pnpm dlx serve .output/public -p 8000 -s",

    "typecheck": "nuxt typecheck",

    "wasm": "asc assembly/polychrome_effect.ts --target release -Ospeed -Osize --converge --noAssert -o app/components/home/polychrome/effect.wasm && asc assembly/polychrome_effect_alt.ts --target release -Ospeed -Osize --converge --noAssert -o app/components/home/polychrome/effect_alt.wasm"
  },
  "type": "module",
  "version": "2.5.2"
}
